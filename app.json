{
  "expo": {
    "name": "Stadium Takeover",
    "slug": "stadium-takeover",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/android.png",
    "scheme": "stadiumtakeover",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": "./assets/images/ios.png",
      "supportsTablet": true,
       "usesAppleSignIn": true,
      "bundleIdentifier": "com.stadiumtakeover.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.stadiumtakeover.app",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/light-logo.png"
    },
    "plugins": [
      "expo-router",
      "expo-apple-authentication",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/light-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#00338D"
        }
      ],
      "expo-video",
      "expo-web-browser",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "defaultChannel": "default",

          "enableBackgroundRemoteNotifications": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.785465840386-ts6voja5vpakp8a8oeo82cm0vaetdg52"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "7450ae98-418f-49ed-b4e2-f78e12146d46"
      }
    },
    "owner": "stadiumtakeover"
  }
}
